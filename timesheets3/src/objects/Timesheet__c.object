    <validationRules>
        <fullName>Description_if_Type_is_Other</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Type__c , &quot;Other&quot;),ISBLANK( Other_description__c ))</errorConditionFormula>
        <errorDisplayField>Other_description__c</errorDisplayField>
        <errorMessage>Description is required for type Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Hour_rate_under_100</fullName>
        <active>true</active>
        <errorConditionFormula>Hour_rate__c  &gt; 100.00</errorConditionFormula>
        <errorDisplayField>Hour_rate__c</errorDisplayField>
        <errorMessage>Hour rate it cant be over $100.00</errorMessage>
    </validationRules>
